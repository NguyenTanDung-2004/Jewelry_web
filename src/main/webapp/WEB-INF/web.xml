		<servlet-name>account</servlet-name>
		<servlet-class>servlet.servlet_account</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>account</servlet-name>
		<url-pattern>/account</url-pattern>
	</servlet-mapping>

	<!-- servlet_account_receive_request -->
	<servlet>
		<servlet-name>servlet_account_receive_request</servlet-name>
