<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Jewelry_web</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <!-- servlet home -->
	<servlet>
		<servlet-name>home</servlet-name>
		<servlet-class>servlet.servlet_home</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<!-- servlet_home_receive_request -->
	<servlet>
		<servlet-name>servlet_home_receive_request</servlet-name>
		<servlet-class>servlet.servlet_home_receive_request</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>servlet_home_receive_request</servlet-name>
		<url-pattern>/servlet_home_receive_request</url-pattern>
	</servlet-mapping>
	
	<!-- servlet product -->
	<servlet>
		<servlet-name>product</servlet-name>
		<servlet-class>servlet.servlet_product</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>product</servlet-name>
		<url-pattern>/product</url-pattern>
	</servlet-mapping>
	
	<!--  servlet_product_receive_request_from_home -->
	<servlet>
		<servlet-name>servlet_product_receive_request_from_home</servlet-name>
		<servlet-class>servlet.servlet_product_receive_request_from_home</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>servlet_product_receive_request_from_home</servlet-name>
		<url-pattern>/servlet_product_receive_request_from_home</url-pattern>
	</servlet-mapping>
	
	<!--  servlet_product_receive_request_from_home -->
	<servlet>
		<servlet-name>servlet_product_from_product</servlet-name>
		<servlet-class>servlet.servlet_product_from_product</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>servlet_product_from_product</servlet-name>
		<url-pattern>/servlet_product_from_product</url-pattern>
	</servlet-mapping>
	
</web-app>